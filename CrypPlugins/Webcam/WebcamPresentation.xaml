<UserControl x:Class="Webcam.WebcamPresentation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:webcam="clr-namespace:Cryptool.Plugins.Webcam" xmlns:local="clr-namespace:Webcam"
             xmlns:webCam="clr-namespace:Webcam"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Image Name="Picture" Grid.Row="0" Grid.Column="0" Width="Auto" Height="Auto"/>
    </Grid>
</UserControl>
