<UserControl x:Class="WebService.WebServicePresentation"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   Height="Auto" Width="Auto" MinWidth="640" MinHeight="480">
    <Grid>
       <TabControl Name="tabControl1" BorderBrush="Transparent" Background="AliceBlue">
            <TabItem Header="Compiler" Name="tabQuellcode" IsSelected="True" Background="AliceBlue">
                <Grid>


                    <Button Height="23" Margin="16.67,0,0,107.549" Name="compileButton" VerticalAlignment="Bottom" Click="compileButton_Click" ToolTip="Kompiliert die erstellte Methode" HorizontalAlignment="Left" Width="70.021">Compile</Button>
                <Label Height="38.341" HorizontalAlignment="Left" Margin="16.67,20,0,0" Name="label1" VerticalAlignment="Top" Width="120" Foreground="DarkBlue" Grid.ColumnSpan="3">Method Header</Label>
                <TextBox Margin="110,0,41.72,30.009" Name="textBox3" IsReadOnly="True" Height="100.02" VerticalAlignment="Bottom" />
                <TextBlock Height="70" Margin="110,0,42.5,0" Name="textBlock1" VerticalAlignment="Top" Background="White"><TextBlock.TextDecorations><TextDecoration Location="OverLine"><TextDecoration.Pen><Pen /></TextDecoration.Pen></TextDecoration></TextBlock.TextDecorations></TextBlock>
                <RichTextBox Margin="109.22,68.592,42.5,190.057" Name="richTextBox1" BorderBrush="White" TextChanged="richTextBox1_TextChanged" IsHitTestVisible="True" Focusable="True" AcceptsTab="True" VerticalScrollBarVisibility="Visible" AutoWordSelection="False" />
                <Label Height="25.005" Margin="0,0,0,75.737" Name="label2" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="108.197">Compiler Result:</Label>
                <Label Height="28" HorizontalAlignment="Left" Margin="16.67,70.014,0,0" Name="label3" VerticalAlignment="Top" Width="120" BorderBrush="Blue" Foreground="Blue" Grid.ColumnSpan="3">Method Body</Label>
                <TextBlock Height="45.728" Margin="110,0,41.72,144.329" Name="textBlock2" VerticalAlignment="Bottom" Background="White" />
                </Grid>
                </TabItem>
            <TabItem Name="tabWsdl" Header="WSDL" IsSelected="True" IsEnabled="True" Background="AliceBlue">
                <Grid><TreeView Margin="0,21.671,80.016,38.341" Name="treeView1" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.VerticalScrollBarVisibility="Visible"></TreeView></Grid>
                
            </TabItem>
            <TabItem IsSelected="True" Name="SignatureTab" Header="Animation" Background="AliceBlue">
                <Grid>
                    <TreeView Margin="0,0,276,9" Name="treeView4" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.HorizontalScrollBarVisibility="Visible" />
                    <Button HorizontalAlignment="Right" Margin="0,0,10,8.662" Name="resetButton" Width="75" Height="23.338" VerticalAlignment="Bottom" Click="resetButton_Click">Reset</Button>
                    <TextBox Margin="0,0.336,10,38" Name="textBox2" HorizontalAlignment="Right" Width="257" TextWrapping="WrapWithOverflow" VerticalScrollBarVisibility="Auto" />
                    <Button Height="23" HorizontalAlignment="Right" Margin="0,0,192,9" Name="button1" VerticalAlignment="Bottom" Width="75" Click="button1_Click_3">Play</Button>
                </Grid>
            </TabItem>
            <TabItem Name="traceTab" IsSelected="True" Header="WS-Security Trace" Background="AliceBlue">
                <TextBox Name="txtTrace" FontFamily="Courier New" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" />
            </TabItem>
        </TabControl>
        <Slider Height="38.341" Margin="0,0,5,0" Name="slider1" VerticalAlignment="Top" IsSelectionRangeEnabled="False" IsMoveToPointEnabled="False" IsDirectionReversed="False" ValueChanged="slider1_ValueChanged" HorizontalAlignment="Right" Width="221.711" />
    </Grid>
</UserControl>
