<UserControl x:Class="Cryptool.MD5.Presentation.Displays.CompressionVariableDisplay"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:displays="clr-namespace:Cryptool.MD5.Presentation.Displays" Height="74" Width="580">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <displays:LabelledIntegerDisplay Grid.Column="0" Caption="A" Value="{Binding A}"/>
        <displays:LabelledIntegerDisplay Grid.Column="1" Caption="B" Value="{Binding B}"/>
        <displays:LabelledIntegerDisplay Grid.Column="2" Caption="C" Value="{Binding C}"/>
        <displays:LabelledIntegerDisplay Grid.Column="3" Caption="D" Value="{Binding D}"/>
    </Grid>
</UserControl>
