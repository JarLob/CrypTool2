<UserControl x:Class="Cryptool.MD5.Presentation.Displays.HashAccumulatorDisplay"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:displays="clr-namespace:Cryptool.MD5.Presentation.Displays" Height="74" Width="580">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <displays:LabelledIntegerDisplay Grid.Column="0" Caption="H1" Value="{Binding H1}"/>
        <displays:LabelledIntegerDisplay Grid.Column="1" Caption="H2" Value="{Binding H2}"/>
        <displays:LabelledIntegerDisplay Grid.Column="2" Caption="H3" Value="{Binding H3}"/>
        <displays:LabelledIntegerDisplay Grid.Column="3" Caption="H4" Value="{Binding H4}"/>
    </Grid>
</UserControl>
