<UserControl x:Class="Cryptool.MD5.Presentation.States.FinishedCompressionPresentation"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="378" Width="570" xmlns:my="clr-namespace:Cryptool.MD5.Presentation.Displays">
    <Grid>
        <my:HashAccumulatorDisplay DataContext="{Binding LastState}" Height="50" Margin="10,10,12,0" VerticalAlignment="Top" />
        <my:CompressionVariableDisplay DataContext="{Binding LastState}" Margin="10,101,12,0" Height="50" VerticalAlignment="Top" />
        <my:HashAccumulatorDisplay DataContext="{Binding CurrentState}" Height="50" Margin="10,191,12,0" VerticalAlignment="Top">
        </my:HashAccumulatorDisplay>
        <Label Height="28" HorizontalContentAlignment="Center" Margin="10,67,12,0" Name="label1" VerticalAlignment="Top" VerticalContentAlignment="Center">+</Label>
        <Label HorizontalContentAlignment="Center" Margin="10,157,12,0" Name="label2" Height="28" VerticalAlignment="Top">=</Label>
        <TextBlock Height="21" Margin="12,0,12,100" Name="textBlock1" VerticalAlignment="Bottom" Text="We have finished the compression and updated our accumulators." />
    </Grid>
</UserControl>
