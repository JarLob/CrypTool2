      </setting>
      <setting name="LastPath" serializeAs="String">
        <value />
      </setting>
      <setting name="ReopenLastFiles" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="logWindowAutoHide" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="DemoInterval" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="UpdateFlag" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowRibbonBar" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowAlgorithmsNavigation" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowAlgorithmsSettings" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="FixedWorkspace" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="ShowBalloonLogMessagesInBalloon" serializeAs="String">
        <value>True</value>
