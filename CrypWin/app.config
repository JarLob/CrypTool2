      </setting>
      <setting name="AutoInstall" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="CheckOnStartup" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="CheckPeriodically" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="CheckInterval" serializeAs="String">
        <value>30</value>
      </setting>
      <setting name="Culture" serializeAs="String">
        <value />
      </setting>
      <setting name="StopWorkspaces" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="RunInBackground" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="ShowInfoLogMessagesInBalloon" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="ShowWarningLogMessagesInBalloon" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="ShowDebugLogMessagesInBalloon" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="BallonVisibility_ms" serializeAs="String">
        <value>2000</value>
      </setting>
      <setting name="LogVisibility" serializeAs="String">
        <value>Visible</value>
      </setting>
      <setting name="SettingVisibility" serializeAs="String">
        <value>Visible</value>
      </setting>
      <setting name="PluginVisibility" serializeAs="String">
        <value>Visible</value>
      </setting>
      <setting name="useDefaultEditor" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="preferredEditor" serializeAs="String">
        <value>WorkspaceManager.WorkspaceManagerClass</value>
      </setting>
      <setting name="useLastPath" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ReopenLastFile" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowStartcenter" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="SamplesDir" serializeAs="String">
        <value>Templates</value>
      </setting>
    </Cryptool.CrypWin.Properties.Settings>
  </userSettings>

  <!-- Load assemblies from given subdir -->
  <runtime>    
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Lib;LibP2P;CrypPlugins"/>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
