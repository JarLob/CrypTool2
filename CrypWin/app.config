<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings">
      <section name="Cryptool.CrypWin.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <userSettings>
    <Cryptool.CrypWin.Properties.Settings>
      <setting name="defaultEditor" serializeAs="String">
        <value>WorkspaceManager.WorkspaceManagerClass</value>
      </setting>
      <setting name="RelHeight" serializeAs="String">
        <value>0.9</value>
      </setting>
      <setting name="RelWidth" serializeAs="String">
        <value>0.9</value>
      </setting>
      <setting name="IsWindowMaximized" serializeAs="String">
