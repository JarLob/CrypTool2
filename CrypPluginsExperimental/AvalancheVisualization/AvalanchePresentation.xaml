<UserControl x:Class="AvalancheVisualization.AvalanchePresentation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AvalancheVisualization"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d" 
             MinHeight="400" MinWidth="650" IsEnabled="True">
    <UserControl.Resources>

        <Style x:Key="buttonStyle" TargetType="Button">
            <Setter Property="Background" Value="#534b4f"/>
            <Setter Property="FontStyle" Value="Normal"/>
            <Setter Property="Margin" Value="1"/>
            <Setter Property="FontSize" Value="10"/>
            <Setter Property="Foreground" Value="White" />
            <Setter Property="BorderBrush" Value="{x:Null}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border CornerRadius="10" Background="#534b4f"  Padding="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="{x:Null}" />
                    <Setter Property="BorderBrush" Value="{x:Null}" />
                    <Setter Property="FontWeight" Value="Bold" />
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="textBlockStyle" TargetType="TextBlock">
            <Setter Property="FontStyle" Value="Normal"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="15"/>
        </Style>

        <Style x:Key="RadioButtonStyle" TargetType="RadioButton">
            <Setter Property="FontStyle" Value="Normal"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="10"/>
        </Style>

        <Style x:Key="LineButton" TargetType="{x:Type RepeatButton}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RepeatButton}">
                        <Border Background="#534b4f" BorderBrush="#534b4f" 
            BorderThickness="1" CornerRadius="5">
                            <Path HorizontalAlignment="Center" 
              VerticalAlignment="Center" 
              Data="{Binding Path=Content,
                     RelativeSource={RelativeSource TemplatedParent}}" 
              Fill="white" >
                            </Path>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="PageButton" TargetType="{x:Type RepeatButton}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RepeatButton}">
                        <Border Background="#66000000" BorderBrush="#66000000"
                         BorderThickness="1" CornerRadius="3" Opacity="0.5">
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="ScrollThumb" TargetType="{x:Type Thumb}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Thumb}">
                        <Grid>
                            <Rectangle RadiusX="3" RadiusY="3" Fill="#534b4f">
                            </Rectangle>
                            <Border CornerRadius="3" BorderBrush="#534b4f">
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="VerticalScrollBar" TargetType="{x:Type ScrollBar}">
            <Setter Property="OverridesDefaultStyle" Value="True"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="18"></RowDefinition>
                                <RowDefinition Height="0.00001*"></RowDefinition>
                                <RowDefinition Height="18"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Border Grid.RowSpan="3" Width="10" CornerRadius="3" BorderBrush="#534b4f" 
                                    BorderThickness="1" Opacity="0.6">
                            </Border>
                            <RepeatButton Grid.Row="0" Style="{StaticResource LineButton}" Height="18" Width="10" 
                                          Command="ScrollBar.LineUpCommand" Content="M 0 4 L 8 4 L 4 0 Z">
                            </RepeatButton>
                            <Track Name="PART_Track" Grid.Row="1" IsDirectionReversed="True">
                                <Track.DecreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" Width="10" Command="ScrollBar.PageUpCommand" />
                                </Track.DecreaseRepeatButton>
                                <Track.Thumb>
                                    <Thumb Style="{StaticResource ScrollThumb}" Width="10" />
                                </Track.Thumb>
                                <Track.IncreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" Width="10" Command="ScrollBar.PageDownCommand" />
                                </Track.IncreaseRepeatButton>
                            </Track>
                            <RepeatButton Grid.Row="2" Style="{StaticResource LineButton}" Height="18" Width="10" 
                                          Command="ScrollBar.LineDownCommand" Content="M 0 0 L 4 4 L 8 0 Z">
                            </RepeatButton>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="HorizontalScrollBar" TargetType="{x:Type ScrollBar}">
            <Setter Property="OverridesDefaultStyle" Value="True"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="18"></ColumnDefinition>
                                <ColumnDefinition Width="0.00001*"></ColumnDefinition>
                                <ColumnDefinition Width="18"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border Grid.ColumnSpan="3" CornerRadius="3" BorderBrush="#534b4f" BorderThickness="1" Opacity="0.6" >
                            </Border>
                            <RepeatButton Grid.Column="0" 
                      Style="{StaticResource LineButton}" 
                      Width="18" Height="13" Command="ScrollBar.LineLeftCommand" 
                      Content="M 4 0 L 4 8 L 0 4 Z">
                            </RepeatButton>
                            <Track Name="PART_Track" Grid.Column="1" IsDirectionReversed="False">
                                <Track.DecreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" 
                              Command="ScrollBar.PageLeftCommand" />
                                </Track.DecreaseRepeatButton>
                                <Track.Thumb>
                                    <Thumb Style="{StaticResource ScrollThumb}" />
                                </Track.Thumb>
                                <Track.IncreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" 
                              Command="ScrollBar.PageRightCommand" />
                                </Track.IncreaseRepeatButton>
                            </Track>
                            <RepeatButton Grid.Column="2" 
                      Style="{StaticResource LineButton}"
                      Width="18" Height="13" Command="ScrollBar.LineRightCommand"
                      Content="M 0 0 L 4 4 L 0 8 Z">
                            </RepeatButton>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="RightScrollViewer" TargetType="{x:Type ScrollViewer}">
            <Setter Property="OverridesDefaultStyle" Value="True" />

            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollViewer}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Border BorderThickness="0,1,1,1" Grid.Column="1">
                                <Border.BorderBrush>
                                    <SolidColorBrush Color="{DynamicResource BorderMediumColor}" />
                                </Border.BorderBrush>
                                <ScrollContentPresenter />
                            </Border>
                            <ScrollBar x:Name="PART_VerticalScrollBar"
                     Grid.Column="2"
                     Style="{StaticResource VerticalScrollBar}"
                     Value="{TemplateBinding VerticalOffset}"
                     Maximum="{TemplateBinding ScrollableHeight}"
                     ViewportSize="{TemplateBinding ViewportHeight}"
                     Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}"/>
                            <ScrollBar x:Name="PART_HorizontalScrollBar"
                     Orientation="Horizontal"
                     Grid.Row="1"
                     Grid.Column="1"
                     Style="{StaticResource HorizontalScrollBar}"
                     Value="{TemplateBinding HorizontalOffset}"
                     Maximum="{TemplateBinding ScrollableWidth}"
                     ViewportSize="{TemplateBinding ViewportWidth}"
                     Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </UserControl.Resources>

    <Grid Name="MainGrid" >

        <Grid.Background>
            <LinearGradientBrush EndPoint="0,0" StartPoint="0,1">
                <GradientStop Color="#1ab3ff" Offset="1"/>
                <GradientStop Color="#f8f8ff" Offset="0"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Grid.RowDefinitions>

            <RowDefinition Height="210"  />
            <RowDefinition Height="225" />

        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="210"/>
            <ColumnDefinition Width="210"/>
            <ColumnDefinition Width="*"/>

        </Grid.ColumnDefinitions>
        <Canvas Name="StartCanvas"  Grid.RowSpan="2" Grid.ColumnSpan="3" Visibility="Visible">
            <TextBlock Name="StartImage" Style="{StaticResource textBlockStyle}" Text="VISUALIZATION OF THE AVALANCHE EFFECT" TextAlignment="Center" TextWrapping="Wrap" FontSize="50"  Width="569" Height="383" Canvas.Left="56" Canvas.Top="52">
                <TextBlock.BitmapEffect>
                    <DropShadowBitmapEffect  Direction="330" Color="Black" ShadowDepth="5" Opacity="0.5" Softness="0.30" />
                </TextBlock.BitmapEffect>
            </TextBlock>
            <TextBlock Name="initExplanationText" Canvas.Top="200" Canvas.Left="135" Width="400" Foreground="#534b4f" FontSize="16" Text="A good avalanche effect is achieved when a small change in the input text results in a significant change in the encrypted text.&#xa;You can select in the settings the category of the algorithm whose avalanche effect you would like to test." TextAlignment="Justify" TextWrapping="Wrap" Visibility="Visible">
            </TextBlock>


        </Canvas>

        <Button x:Name="afterInitRoundButton" Style="{StaticResource buttonStyle}" Width="100" Height="20" Content=" After initial round" Grid.Row="0"  VerticalAlignment="Top" HorizontalAlignment="Center"   Click="afterInitRoundButton_Click" Visibility="Hidden" />
        <!-- <Button x:Name="goBackButton" Style="{StaticResource buttonStyle}" Width="60" Height="35" Content="Go back"  Grid.Row="1"   VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,10,0,30" Click="goBackButton_Click"  Visibility="Hidden"/>-->
       

        <Button x:Name="doneButton" Style="{StaticResource buttonStyle}" Grid.Row="1" Grid.Column="1"  Width="60" Height="25" Content="done" FontSize="13" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Margin="20,45" Visibility="Hidden" Click="doneButton_Click"/>
        <Button x:Name="clearButton" Style="{StaticResource buttonStyle}" Grid.Row="1" Grid.Column="2"  Width="60" Height="25" Content="clear" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,45" Visibility="Hidden" Click="clearButton_Click"/>
        <!--<ScrollViewer x:Name="buttonsSV"  Grid.Row="1" Grid.ColumnSpan="3"                     
                      Style="{StaticResource RightScrollViewer}"  
                      Width="640" Height="60" Margin="0,30" 
                      VerticalAlignment="Bottom"
                      HorizontalScrollBarVisibility="Hidden"  
                      VerticalScrollBarVisibility="Disabled" Visibility="Hidden" >-->
        <!-- <ScrollViewer.Resources>
              <sys:Double x:Key="{x:Static SystemParameters.VerticalScrollBarWidthKey}">8</sys:Double>
              </ScrollViewer.Resources>-->

        <StackPanel x:Name="buttonsPanel" Orientation="Horizontal" Grid.Row="1" Grid.ColumnSpan="3"  VerticalAlignment="Bottom" Margin="10,20" Visibility="Hidden">

            <Button x:Name="afterRound0Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="0"  Click="afterRound0Button_Click" />
            <Button x:Name="afterRound1Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="1"  Click="afterRound1Button_Click" />
            <Button x:Name="afterRound2Button"  Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="2"   Click="afterRound2Button_Click" />
            <Button x:Name="afterRound3Button" Style="{StaticResource buttonStyle}"  Width="30" Height="30" Content="3" Click="afterRound3Button_Click" />
            <Button x:Name="afterRound4Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="4"   Click="afterRound4Button_Click" />
            <Button x:Name="afterRound5Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="5"   Click="afterRound5Button_Click" />
            <Button x:Name="afterRound6Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="6"  Click="afterRound6Button_Click" />
            <Button x:Name="afterRound7Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="7" Click="afterRound7Button_Click" />
            <Button x:Name="afterRound8Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="8" Click="afterRound8Button_Click" />
            <Button x:Name="afterRound9Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="9"  Click="afterRound9Button_Click" />
            <Button x:Name="afterRound10Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="10"  Click="afterRound10Button_Click"></Button>
            <Button x:Name="afterRound11Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="11"  Click="afterRound11Button_Click" Visibility="Collapsed"></Button>
            <Button x:Name="afterRound12Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="12"  Click="afterRound12Button_Click" Visibility="Collapsed"></Button>
            <Button x:Name="afterRound13Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="13"  Click="afterRound13Button_Click" Visibility="Collapsed"></Button>
            <Button x:Name="afterRound14Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="14"  Click="afterRound14Button_Click" Visibility="Collapsed"></Button>
            <Button x:Name="afterRound15Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="15"  Click="afterRound15Button_Click" Visibility="Collapsed"></Button>
            <Button x:Name="afterRound16Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="16"  Click="afterRound16Button_Click" Visibility="Collapsed"></Button>
            <Line x:Name="line" Stroke="#008000" StrokeThickness="3" X1="0" Y1="0" X2="10" Y2="30" Visibility="Visible">
                <Line.BitmapEffect>
                    <DropShadowBitmapEffect Color="Black" Direction="300" ShadowDepth="5" Opacity="0.8" Softness="0.4"  />
                </Line.BitmapEffect>
            </Line>
            <Button x:Name="inputDataButton" Style="{StaticResource buttonStyle}" Width="60" Height="30" Content="Input data" Click="inputDataButton_Click"/>
            <Button x:Name="overviewButton" Style="{StaticResource buttonStyle}" Width="60" Height="30" FontSize="8" Content=" General&#xa; overview"/>
        </StackPanel>



        <TextBlock Name="showByteDependencyText" Grid.ColumnSpan="2" Grid.RowSpan="2" Text="Check how many byte changes are triggered by a single change in an input byte." FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Justify" TextWrapping="Wrap" Width="230" Margin="0,30,60,0" Visibility="Hidden">
        </TextBlock>
        <TextBlock Name="avalancheEffectAESText" Grid.Column="1" Grid.ColumnSpan="2" Grid.RowSpan="2" Text="Check how strong the avalanche effect is in every single AES-128 round." FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Justify" TextWrapping="Wrap" Width="230" Margin="30,0,0,0" Visibility="Hidden">
        </TextBlock>
        <TextBlock Name="comparisonTxtBlock" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" Foreground="#534b4f" Width="250" Margin="30" FontSize="14" TextWrapping="Wrap" TextAlignment="Center" Visibility="Hidden">You can modify the original input connected to the algorithm's plugin.</TextBlock>
        <TextBlock Name="buttonsTitle" Grid.Row="1" VerticalAlignment="Bottom" Foreground="#534b4f" FontWeight="DemiBold" Text="Check avalanche effect after round" Visibility="Hidden" Margin="10,55">
        </TextBlock>
        <TextBlock Name="initStateTitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" HorizontalAlignment="Center" Text="Input data AES-" Visibility="Hidden" >
        </TextBlock>
        <TextBlock Name="afterRoundsTitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" HorizontalAlignment="Center" TextAlignment="Center" Visibility="Hidden" >
        </TextBlock>
        <TextBlock Name="afterRoundsSubtitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="20" Foreground="#534b4f" HorizontalAlignment="Center" TextAlignment="Center" Text="after round "  Visibility="Hidden" Margin="0,30" >
        </TextBlock>
        <Line x:Name="divideLine" Grid.Column="1" Grid.RowSpan="2" Stroke="White" StrokeThickness="5" X1="110" Y1="150" X2="110" Y2="300" Visibility="Hidden">
            <Line.BitmapEffect>
                <DropShadowBitmapEffect Color="Black" Direction="300" ShadowDepth="5" Opacity="0.8" Softness="0.4"  />
            </Line.BitmapEffect>
        </Line>

        <!-- -->

        <local:PieChart  x:Name="flippedBitsPiece" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Width="170" Height="170" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="80,15" >
            <local:PieChart.ToolTip>
                <ToolTip x:Name="tp1" Height="40" Background="#6082b6" Foreground="White" FontWeight="Bold">
                </ToolTip>
            </local:PieChart.ToolTip>
            <local:PieChart.Effect>
                <DropShadowEffect/>
            </local:PieChart.Effect>

        </local:PieChart>

        <local:PieChart  x:Name="unflippedBitsPiece" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Width="170" Height="170" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="80,15" >
            <local:PieChart.ToolTip>
                <ToolTip x:Name="tp2" Height="40" Background="#6082b6" Foreground="White" FontWeight="Bold">
                </ToolTip>
            </local:PieChart.ToolTip>
            <local:PieChart.Effect>
                <DropShadowEffect/>
            </local:PieChart.Effect>
        </local:PieChart>

        <Canvas Name="curvedLinesCanvas" Visibility="Hidden">
            <Path Stroke="#534b4f" StrokeThickness="3">
                <Path.Data>
                    <PathGeometry>
                        <PathGeometry.Figures>
                            <PathFigure StartPoint="185,229">
                                <!--<BezierSegment Point1="180,180" Point2="185,200" Point3="230,155"></BezierSegment>-->
                                <QuadraticBezierSegment Point1="180,180" Point2="240,161"/>
                            </PathFigure>
                        </PathGeometry.Figures>
                    </PathGeometry>
                </Path.Data>
            </Path>

            <Path Stroke="#534b4f" StrokeThickness="3">
                <Path.Data>
                    <PathGeometry>
                        <PathGeometry.Figures>
                            <PathFigure StartPoint="189,80">
                                <QuadraticBezierSegment Point1="190,110" Point2="240,130"></QuadraticBezierSegment>
                                <!--<QuadraticBezierSegment Point1="195,150" Point2="230,120"/>-->
                            </PathFigure>
                        </PathGeometry.Figures>
                    </PathGeometry>
                </Path.Data>
            </Path>

            <Polygon Points="240,120 250,130, 240,140" StrokeThickness="2"  Stroke="#534b4f" Fill="#007fff" Visibility="Visible" >
            </Polygon>
            <Polygon Points="240,151 250,161, 240,171" StrokeThickness="2"  Stroke="#534b4f" Fill="#007fff" Visibility="Visible" >
            </Polygon>
        </Canvas>

        <Polygon Name="arrow1" Points="175,125 190,140, 175,155" Width="25" Height="35" StrokeThickness="3" Stroke="#4cbb17" Stretch="Fill" HorizontalAlignment="Right"  Visibility="Hidden"  Margin="20,70,0,0">
        </Polygon>
        <Polygon Name="arrow2" Grid.Row="1" Points="175,125 190,140, 175,155" Width="25" Height="35" StrokeThickness="3" Stroke="#4cbb17" Stretch="Fill" HorizontalAlignment="Right" VerticalAlignment="Top" Visibility="Hidden"  Margin="20,50,0,0">
        </Polygon>
        <CheckBox Name="aesCheckBox" Grid.Row="1"  Foreground="#534b4f" Content="Enable single bit changes"  Width="160" VerticalAlignment="Bottom" Checked="checkBox_Checked" Unchecked="checkBox_Unchecked" Margin="20,50" Visibility="Hidden"/>


        <StackPanel Name="radioButtons" Grid.Column="2" Grid.RowSpan="2"  VerticalAlignment="Center" HorizontalAlignment="Center"  Visibility="Hidden">
            <TextBlock Foreground="#534b4f" FontSize="14" Padding="5">Display information in
            </TextBlock>
            <StackPanel Name="radioButtonPanel" Orientation="Horizontal" Margin="10,0">
               <!-- <RadioButton Name="radioBinary" Foreground="#534b4f" FontSize="10"  Padding="0" Checked="radioButton1Checked" Content="Binary"></RadioButton>-->
                <RadioButton Name="radioDecimal" Foreground="#534b4f" FontSize="10"  Padding="0" Margin="10,0" Checked="radioButton2Checked" Content="Decimal">
                </RadioButton>
                <RadioButton Name="radioHexa" Foreground="#534b4f" FontSize="10" Padding="0" Margin="10,0" Checked="radioButton3Checked"  Content="Hexadecimal">
                </RadioButton>
            </StackPanel>
        </StackPanel>


        <Grid Name="othersGrid" Grid.ColumnSpan="3" Grid.RowSpan="2" Visibility="Hidden">
            <Grid.RowDefinitions>
                <RowDefinition Height="60"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="110"/>
                <RowDefinition />
            </Grid.RowDefinitions>
            
            <TextBlock Name="explanationTxt_others"  Width="160" Height="50" TextWrapping="Wrap" TextAlignment="Justify" Text="Input changes can be done by directly modifying the input plugin."  Foreground="#534b4f"  HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Row="3" Margin="12,15"></TextBlock>
            <TextBlock Name="title_Other"  Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" HorizontalAlignment="Center" Text="Avalanche effect ">
            </TextBlock>
            <TextBlock Grid.Row="1" Text="Input Data" HorizontalAlignment="Center" Foreground="#534b4f" FontSize="17" FontWeight="DemiBold"/>
            <TextBlock Name="othersSubtitle" Text="(Classic ciphers)" Foreground="#534b4f" FontSize="15" FontWeight="Light" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,18,0,0"></TextBlock>
            <TextBlock Name="othersOrigTitle"  Grid.Row="1"  Style="{StaticResource textBlockStyle}" Foreground="#6e7f80" FontSize="10" Text="Encryption original message" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="150,0,0,0">
            </TextBlock>

            <TextBlock Name="othersModTitle" Grid.Row="1" Grid.Column="1"  Style="{StaticResource textBlockStyle}" Foreground="#6e7f80"  FontSize="10" Text="Encryption modified message" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="240,0,0,0">
            </TextBlock>

            <TextBlock Name="lbl" Grid.Row="3" Text="Encryption original message" Foreground="#534b4f" Margin="20"></TextBlock>
            <TextBlock Name="lbl2" Grid.Row="3" Text="Encryption modified message" Foreground="#534b4f" Margin="20,45,0,0"></TextBlock>
            <TextBlock Name="lbl3" Grid.Row="3" Text="Changed bits" Foreground="#534b4f" Margin="20,70,0,0"></TextBlock>
            
            <StackPanel Grid.Row="3" x:Name="bitComparisonStkPanel" Height="100" Margin="0,20,0,0">
                <StackPanel.BitmapEffect>
                    <DropShadowBitmapEffect Color="Black" Direction="260" ShadowDepth="5" Opacity="0.5" Softness="0.6"  />
                </StackPanel.BitmapEffect>

                

                <ScrollViewer  Name="bar1" Width="300" Height="25" ScrollChanged="ScrollViewer_ScrollChanged"   
                           VerticalScrollBarVisibility="Disabled"  HorizontalScrollBarVisibility="Hidden" Style="{StaticResource RightScrollViewer}">
                    <TextBox x:Name="TB1" FontFamily="Courier New"  FontSize="20" FontWeight="Medium"  IsReadOnly="True" />
                </ScrollViewer>
                <ScrollViewer Name="bar2" Width="300" Height="25"  ScrollChanged="ScrollViewer_ScrollChanged" 
                          VerticalScrollBarVisibility="Disabled" HorizontalScrollBarVisibility="Hidden" Style="{StaticResource RightScrollViewer}">
                    <TextBox Name="TB2" FontFamily="Courier New" FontSize="20" FontWeight="Medium" IsReadOnly="True"  />
                </ScrollViewer>

                <ScrollViewer Name="bar3" Height="38" Width="300" ScrollChanged="ScrollViewer_ScrollChanged"
                          VerticalScrollBarVisibility="Disabled" HorizontalScrollBarVisibility="Visible" Style="{StaticResource RightScrollViewer}">
                    <TextBox x:Name="TB3"  FontFamily="Courier New" FontSize="20" FontWeight="Medium" Foreground="Red" IsReadOnly="True" />
                </ScrollViewer>
            </StackPanel>

            <StackPanel Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Left" Margin="10,20,0,0" >
                <TextBlock Foreground="#534b4f" FontSize="12" Padding="5">Display info. in
                </TextBlock>
                <StackPanel >
                    <!-- <RadioButton Name="radioBinary" Foreground="#534b4f" FontSize="10"  Padding="0" Checked="radioButton1Checked" Content="Binary"></RadioButton>-->
                    <RadioButton Foreground="#534b4f" FontSize="10"  Padding="0" Margin="10,0" Checked="radioButton2Checked" Content="Decimal">
                    </RadioButton>
                    <RadioButton Foreground="#534b4f" FontSize="10" Padding="0" Margin="10,0" Checked="radioButton3Checked"  Content="Hexadecimal">
                    </RadioButton>
                </StackPanel>
            </StackPanel>


            <Grid Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>

              

                    <TextBox Name="originalMsg" Width="200" IsReadOnly="True" TextWrapping="Wrap" HorizontalAlignment="Right" Margin="0,0,20,0" VerticalScrollBarVisibility="Auto">
                </TextBox>

                <TextBox Grid.Column="2" Width="200" Name="modifiedMsg"  TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Left" Margin="20,0,0,0" VerticalScrollBarVisibility="Auto">
                </TextBox>


            </Grid>
            
        </Grid>





        <Grid Name="OrigInitialStateGrid"  Grid.RowSpan="2"  Visibility="Hidden" Margin="0,70">

