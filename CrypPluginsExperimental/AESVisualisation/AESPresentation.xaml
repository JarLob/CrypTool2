            <Grid Grid.Row="1" Grid.Column="7" Grid.ColumnSpan="2" Margin="0,20,0,30">
                <Viewbox Stretch="Uniform" >
                    <TextBlock  Width="120" Name="expansionExplanation" Visibility="Hidden" Text="First, the last four bytes of the previous key are taken. Afterwards, the last byte is placed at the front." TextWrapping="Wrap"/>
                </Viewbox>
                <Viewbox Stretch="Uniform" >
                    <TextBlock  Width="120" Name="expansionExplanationDE" Visibility="Hidden" Text="Zuerst werden die letzten vier Bytes des vorherigen Schlüssels genommen und das letzte Byte an den Anfang gestellt." TextWrapping="Wrap"/>
                </Viewbox>
            </Grid>
            <Grid Grid.Row="2" Grid.Column="3" Grid.ColumnSpan="2" Margin="0, 20, 0, 30">
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation1" Visibility="Hidden" Text="Every byte is exchanged with the corresponding byte from the sBox" TextWrapping="Wrap" Width="100" />
                </Viewbox>
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation1DE" Visibility="Hidden" Text="Jedes Byte wird mit dem entsprechenden Byte aus der S-Box ausgetauscht." TextWrapping="Wrap" Width="100" />
                </Viewbox>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" Margin="0, 10, 0, 80">
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation2" Visibility="Hidden" Text="A round constant is added" TextWrapping="Wrap" Width="100" />
                </Viewbox>
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation2DE" Visibility="Hidden" Text="Die Rundenkonstante wird addiert." TextWrapping="Wrap" Width="100" />
                </Viewbox>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" Margin="0, 10, 0, 60">
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation3" Visibility="Hidden" Text="Adding the first four bytes of the previous key gives you the first four bytes of the next key." TextWrapping="Wrap" Width="120" />
                </Viewbox>
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation3DE" Visibility="Hidden" Text="Die ersten vier Bytes des vorherigen Schlüssels werden addiert. Das Ergebnis sind die ersten vier Bytes des neuen Schlüssels." TextWrapping="Wrap" Width="120" />
                </Viewbox>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" Margin="0, 10, 0, 60">
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation4" Visibility="Hidden" Text="For column x of the new key you XOR column x from the previous key with column x-1 from the new key." TextWrapping="Wrap" Width="120" />
                </Viewbox>
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="expansionExplanation4DE" Visibility="Hidden" Text="Für Spalte x des neuen Schlüssels wird Spalte x des vorherigen Schlüssels mit Spalte x-1 des neuen Schlüssels per XOR verrechnet." TextWrapping="Wrap" Width="120" />
                </Viewbox>
            </Grid>
            <!--EndRegion-->
            <!--Region MixCol-->
            <Grid Grid.Row="1" Grid.Column="7" Grid.ColumnSpan="3" Margin="0, 10, 0, 60">
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="mixColExplanation" Visibility="Hidden" Text="One column is taken from the current state and placed next to the multiplication matrix." TextWrapping="Wrap" Width="120" />
                </Viewbox>
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="mixColExplanationDE" Visibility="Hidden" Text="Eine Spalte des aktuellen Zustandes wird genommen und neben der Multiplikationsmatrix platziert." TextWrapping="Wrap" Width="120" />
                </Viewbox>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="7" Grid.ColumnSpan="3" Margin="0, 10, 0, 60">
                <Viewbox Stretch="Uniform">
                    <TextBlock Name="mixColExplanation1" Visibility="Hidden" Text="Then it is multiplied with the multiplication matrix to determine the next column of the next state." TextWrapping="Wrap" Width="120" />
