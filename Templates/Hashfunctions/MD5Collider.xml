<?xml version="1.0" encoding="utf-8"?>
<sample>
  <title lang="en">MD5 Collision Finder</title>
  <title lang="ru">MD5 Collision Finder</title>
  <summary lang="en">Usage of the <i>MD5 Collision Finder</i></summary>
  <summary lang="ru">Использование MD5 Collision Finder</summary>
  <description lang="en">This template shows the usage of the <i>MD5 Collision Finder</i>.</description>
  <description lang="ru">Этот шаблон показывает использование MD5 Collision Finder.</description>
  <keywords>message, digest, Hash, Rivest, collision</keywords>
  <keywords lang="ru">сообщение, дайджест, хэш, ривест, столкновение</keywords>
  <title lang="de">MD5-Kollisionsfinder</title>
  <summary lang="de">Benutzung des <i>MD5-Kollisionsfinders</i></summary>
  <description lang="de">Diese Vorlage zeigt die Benutzung des <i>MD5-Kollisionsfinders</i>.</description>
  <keywords lang="de">hash</keywords>
  <icon file="MD5.png" />
  <relevantPlugins>
    <plugin name="BooleanOutput" />
    <plugin name="Concatenate" />
    <plugin name="MD5" />
    <plugin name="MD5Collider" />
    <plugin name="StreamComparator" />
  </relevantPlugins>
  <replacements lang="de">
    <replacement key="$Random seed$" value="Zufälliger Seed (optional)" />
    <replacement key="$MD5Collider$" value="MD5-Kollisionsgenerator" />
    <replacement key="$Prefix$" value="Präfix (Vielfaches von 64 Bytes)" />
    <replacement key="$Suffix$" value="Suffix (Daten beliebiger Länge)" />
    <replacement key="$StringDecoder$" value="String-Kodierer" />
    <replacement key="$StringEncoder$" value="String-Dekodierer" />
    <replacement key="$Concatenate$" value="Konkatenation" />
    <replacement key="$MD5 of data$" value="MD5 von Daten" />
    <replacement key="$Data$" value="Daten" />
    <replacement key="$Compare Data$" value="Vergleiche Daten" />
    <replacement key="$Compare Hash$" value="Vergleiche Hash" />
    <replacement key="$Same Data$" value="Gleiche Daten" />
    <replacement key="$Same Hash$" value="Gleicher Hash" />
    <replacement key="$memo$" value="Diese Vorlage demonstriert die Verwendung der Komponente MD5-Kollisionsgenerator, mit der eine MD5-Kollision der Form\line &lt;PRÄFIX&gt; || &lt;GENERIERTER KOLLISIONSBLOCK&gt; || &lt;SUFFIX&gt;\line erzeugt wird." />
  </replacements>
  <replacements lang="ru">
    <replacement key="$Random seed$" value="Случайное семя (необязательно)" />
    <replacement key="$MD5Collider$" value="MD5-Цион Генератор Kolli" />
    <replacement key="$Prefix$" value="Префикс (несколько из 64 байтов)" />
    <replacement key="$Suffix$" value="Суффикс (данные любой длины)" />
    <replacement key="$StringDecoder$" value="Строка кодировщик" />
    <replacement key="$StringEncoder$" value="Строка декодера" />
    <replacement key="$Concatenate$" value="Konkatenation" />
    <replacement key="$MD5 of data$" value="MD5 данных" />
    <replacement key="$Data$" value="данные" />
    <replacement key="$Compare Data$" value="Сравнить данные" />
    <replacement key="$Compare Hash$" value="Сравнить хеш" />
    <replacement key="$Same Data$" value="Те же данные" />
    <replacement key="$Same Hash$" value="Тот же хеш" />
    <replacement key="$memo$" value="Этот шаблон демонстрирует использование компонента генератора столкновений MD5, который вызывает столкновение MD5 формы \line &lt;PRÄFIX&gt; || &lt;GENERIERTER KOLLISIONSBLOCK&gt; || &lt;SUFFIX&gt; \line." />
  </replacements>
  <replacements lang="en">
    <replacement key="$Random seed$" value="Random seed (optional)" />
    <replacement key="$MD5Collider$" value="MD5 Collider" />
    <replacement key="$Prefix$" value="Prefix (must be a multiple of 64 bytes)" />
    <replacement key="$Suffix$" value="Suffix (data of any length)" />
    <replacement key="$StringDecoder$" value="String Decoder" />
    <replacement key="$StringEncoder$" value="String Encoder" />
    <replacement key="$Concatenate$" value="Concatenate" />
    <replacement key="$MD5 of data$" value="MD5 of data" />
    <replacement key="$Data$" value="Data" />
    <replacement key="$Compare Data$" value="Compare Data" />
    <replacement key="$Compare Hash$" value="Compare Hash" />
    <replacement key="$Same Data$" value="Same Data" />
    <replacement key="$Same Hash$" value="Same Hash" />
    <replacement key="$memo$" value="This template demonstrates the use of the MD5Collider component to generate an MD5 collision. A collision of the form\line &lt;PREFIX&gt; || &lt;GENERATED COLLISION BLOCK&gt; || &lt;SUFFIX&gt;\line is created." />
  </replacements>
</sample>