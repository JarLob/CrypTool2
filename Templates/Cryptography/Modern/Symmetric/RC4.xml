    <replacement key="$Receiver Message$" value="Empfänger-Nachricht (entschlüsselt)"/>
    <replacement key="$RC4 Encrypt$" value="RC4 Verschlüsselung"/>
    <replacement key="$RC4 Decrypt$" value="RC4 Entschlüsselung"/>
	 <replacement key="$Hex to Bytes$" value="Hex -> Bytes"/>
    <replacement key="$Keystream$" value="Schlüsselstrom"/>
    <replacement key="$Converter$" value="Konvertierer"/>
    <replacement key="$Encryption$" value="Verschlüsselung"/>
    <replacement key="$Decryption$" value="Entschlüsselung"/>
    <replacement key="$StringEncoder$" value="String-Codierer"/>
    <replacement key="$Transmitted Message (ASCII)$" value="Übertragene Nachricht (ASCII)"/>
    <replacement key="$Transmitted Message (Hex)$" value="Übertragene Nachricht (Hex)"/>
    <replacement key="$Transmission Channel$" value="Übertragungskanal"/>
