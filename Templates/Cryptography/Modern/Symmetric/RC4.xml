  <keywords lang="de">Blockchiffre, symmetrisch</keywords>

  <icon file="RC4.png" />
  <relevantPlugins>
    <plugin name="RC4" />
  </relevantPlugins>

  <replacements lang="de">
    <replacement key="$Sender$" value="Sender"/>
    <replacement key="$Receiver$" value="Empfänger"/>
