<?xml version="1.0" encoding="utf-8"?>
<sample>
  <title lang="en">Keccak Cipher</title>
  <title lang="ru">Keccak Cipher</title>
  <summary lang="en">Usage of <i>Keccak</i> as a stream cipher</summary>
  <summary lang="ru">Использование Keccak в качестве потокового шифрования</summary>
  <description lang="en">This template shows how the <i>Keccak</i> component can be used as a keystream generator.</description>
  <description lang="ru">Этот шаблон показывает, как компонент Keccak может использоваться как генератор ключей.</description>
  <keywords>stream cipher, keccak</keywords>
  <keywords lang="ru">потоковый шифр, keccak</keywords>
  <title lang="de">Keccak-Chiffre</title>
  <summary lang="de">Benutzung von <i>Keccak</i> als Stromchiffre</summary>
  <description lang="de">Diese Vorlage zeigt, wie die <i>Keccak</i>-Komponente als Schlüsselstromgenerator eingesetzt werden kann.</description>
  <keywords lang="de">stromchiffre, keccak</keywords>
  <icon file="Keccak.png" />
  <relevantPlugins>
    <plugin name="Keccak" />
    <plugin name="Xor" />
  </relevantPlugins>
  <replacements lang="de">
    <replacement key="$Sender$" value="Sender" />
    <replacement key="$Receiver$" value="Empfänger" />
    <replacement key="$Sender Key$" value="Sender-Schlüssel" />
    <replacement key="$Receiver Key$" value="Empfänger-Schlüssel" />
    <replacement key="$Keystream$" value="Schlüsselstrom" />
    <replacement key="$Converter$" value="Konvertierer" />
    <replacement key="$Encryption$" value="Verschlüsselung" />
    <replacement key="$Decryption$" value="Entschlüsselung" />
    <replacement key="$StringEncoder$" value="String-Codierer" />
    <replacement key="$Transmitted Message$" value="Übertragene Nachricht" />
    <replacement key="$Transmission Channel$" value="Übertragungskanal" />
    <replacement key="$text$" value="Eine geheime Nachricht für mich selbst.&#xD;&#xA;Dies ist ein Test.&#xD;&#xA;Nachrichten verschicken macht Spaß." />
  </replacements>
  <replacements lang="ru">
    <replacement key="$Sender$" value="отправитель" />
    <replacement key="$Receiver$" value="приемник" />
    <replacement key="$Sender Key$" value="Передатчик-ключ" />
    <replacement key="$Receiver Key$" value="приемник ключ" />
    <replacement key="$Keystream$" value="ключ тока" />
    <replacement key="$Converter$" value="преобразование" />
    <replacement key="$Encryption$" value="кодирование" />
    <replacement key="$Decryption$" value="дешифрование" />
    <replacement key="$StringEncoder$" value="Строка кодировщик" />
    <replacement key="$Transmitted Message$" value="Передано сообщение" />
    <replacement key="$Transmission Channel$" value="канал передачи" />
    <replacement key="$text$" value="Секретное сообщение для себя. Это тест. Отправить сообщения - это весело." />
  </replacements>
  <replacements lang="en">
    <replacement key="$Sender$" value="Sender" />
    <replacement key="$Receiver$" value="Receiver" />
    <replacement key="$Sender Key$" value="Sender Key" />
    <replacement key="$Receiver Key$" value="Receiver Key" />
    <replacement key="$Keystream$" value="Keystream" />
    <replacement key="$Converter$" value="Converter" />
    <replacement key="$Encryption$" value="Encryption" />
    <replacement key="$Decryption$" value="Decryption" />
    <replacement key="$StringEncoder$" value="StringEncoder" />
    <replacement key="$Transmitted Message$" value="Transmitted Message" />
