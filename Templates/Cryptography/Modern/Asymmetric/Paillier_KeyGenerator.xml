<?xml version="1.0" encoding="utf-8"?>
<sample>
  <title lang="en">Paillier Key Generator</title>
  <title lang="ru">Генератор ключей Paillier</title>
  <summary lang="en">Usage of the <i>Pailler</i> cipher with key generation</summary>
  <summary lang="ru">Использование шифра Pailler с генерацией ключей</summary>
  <description lang="en">This template shows the usage of the <i>Pailler</i> cipher with key generation.</description>
  <description lang="ru">Этот шаблон показывает использование шифра Pailler с генерацией ключей.</description>
  <keywords>homomorphic, Damgard, Eurocrypt, Jurik, Paillier, asymmetric</keywords>
  <keywords lang="ru">гомоморфный, Damgard, Eurocrypt, Jurik, Paillier, асимметричный</keywords>
  <title lang="de">Paillier-Schlüsselgenerator</title>
  <summary lang="de">Benutzung der <i>Paillier</i>-Chiffre mit Schlüsselgenerierung</summary>
  <description lang="de">Diese Vorlage zeigt die Benutzung der <i>Paillier</i>-Chiffre mit Schlüsselgenerierung.</description>
  <keywords lang="de">homomorph, asymmetrisch</keywords>
  <icon file="Pailler_KeyGenerator.png" />
  <relevantPlugins>
    <plugin name="Paillier" />
    <plugin name="PaillierKeyGenerator" />
  </relevantPlugins>
  <replacements lang="de">
    <replacement key="$KeyGenerator$" value="Schlüsselgenerator" />
    <replacement key="$Encrypt$" value="Verschlüsselung" />
    <replacement key="$Decrypt$" value="Entschlüsselung" />
    <replacement key="$Plaintext$" value="Klartext" />
    <replacement key="$Ciphertext$" value="Geheimtext" />
    <replacement key="$Input$" value="Eingabe" />
  </replacements>
  <replacements lang="ru">
    <replacement key="$KeyGenerator$" value="генератор ключей" />
    <replacement key="$Encrypt$" value="кодирование" />
    <replacement key="$Decrypt$" value="дешифрование" />
    <replacement key="$Plaintext$" value="обычный текст" />
    <replacement key="$Ciphertext$" value="шифротекста" />
    <replacement key="$Input$" value="вход" />
  </replacements>
  <replacements lang="en">
    <replacement key="$KeyGenerator$" value="KeyGenerator" />
    <replacement key="$Encrypt$" value="Encrypt" />
    <replacement key="$Decrypt$" value="Decrypt" />
    <replacement key="$Plaintext$" value="Plaintext" />
    <replacement key="$Ciphertext$" value="Ciphertext" />
