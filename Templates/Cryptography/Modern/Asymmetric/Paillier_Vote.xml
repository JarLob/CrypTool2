    <replacement key="$Encrypt$" value="кодирование" />
    <replacement key="$Decrypt$" value="дешифрование" />
    <replacement key="$Add$" value="прибавление" />
    <replacement key="$Voting Result$" value="результаты выборов" />
  </replacements>
  <replacements lang="en">
    <replacement key="$Voter$" value="Voter" />
    <replacement key="$Converter$" value="Converter" />
    <replacement key="$KeyGenerator$" value="KeyGenerator" />
    <replacement key="$Encrypt$" value="Encrypt" />
    <replacement key="$Decrypt$" value="Decrypt" />
    <replacement key="$Add$" value="Add" />
    <replacement key="$Voting Result$" value="Voting Result" />
