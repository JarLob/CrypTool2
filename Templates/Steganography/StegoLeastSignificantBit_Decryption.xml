<?xml version="1.0" encoding="utf-8"?>
<sample>

  <title lang="en">Steganography Least Significant Bit Decryption</title>
  <summary lang="en">Decryption with the steganography component <i>StegoLSB</i></summary>
  <description lang="en">This template shows the usage of the steganography component <i>StegoLSB</i> for decryption using the least significant bit.</description>
  <keywords>Steganography, LSB, hidden</keywords>

  <title lang="de">Steganographie Niedrigstwertiges Bit Entschlüsselung</title>
  <summary lang="de">Entschlüsselung mit der Steganographie-Komponente <i>StegoLSB</i></summary>
  <description lang="de">Diese Vorlage zeigt die Benutzung der Steganographie-Komponente <i>StegoLSB</i> zur Entschlüsselung mit niedrigstwertigem Bit.</description>
  <keywords lang="de">Steganographie, verstecken</keywords>

  <icon file="Steganography.png" />

  <relevantPlugins>
    <plugin name="FileInputClass" />
    <plugin name="StegoLeastSignificantBit" />
  </relevantPlugins>

  <replacements lang="de">
    <replacement key="$Carrier Picture$" value="Trägerbild"/>
    <replacement key="$Password$" value="Passwort"/>
    <replacement key="$String Encoder$" value="String-Kodierer"/>
    <replacement key="$Message$" value="Nachricht"/>
  </replacements>
  <replacements lang="en">
    <replacement key="$Carrier Picture$" value="Carrier Picture"/>
    <replacement key="$Password$" value="Password"/>
    <replacement key="$String Encoder$" value="String Encoder"/>
    <replacement key="$Message$" value="Message"/>
  </replacements>

</sample>
